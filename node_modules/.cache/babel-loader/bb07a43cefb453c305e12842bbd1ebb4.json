AACA,MAAA,QAAA,EAAA,EAhBA;AAiBA,MAAA,WAAA,EAAA,KAjBA;AAkBA,MAAA,QAAA,EAAA,EAlBA;AAmBA,MAAA,IAAA,EAAA,CACA;AACA,QAAA,KAAA,EAAA,SADA;AAEA,QAAA,GAAA,EAAA;AAFA,OADA,EAKA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,GAAA,EAAA;AAFA,OALA,EASA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,GAAA,EAAA;AAFA,OATA,EAaA;AACA,QAAA,KAAA,EAAA,YADA;AAEA,QAAA,GAAA,EAAA;AAFA,OAbA,EAiBA;AACA,QAAA,KAAA,EAAA,SADA;AAEA,QAAA,GAAA,EAAA;AAFA,OAjBA;AAnBA,KAAA;AA0CA,GA5CA;AA8CA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,UAAA,KAAA,QAAA,KAAA,EAAA,EAAA;AACA;AACA;;AACA,WAAA,IAAA,CAAA,IAAA,CAAA;AACA,QAAA,KAAA,EAAA,KAAA,QADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA;AAIA,KATA;AAUA,IAAA,OAVA,qBAUA;AAAA;AACA;AACA,UAAA,KAAA,SAAA,KAAA,EAAA,EAAA;AACA;AACA;;AACA,UAAA,IAAA,CALA,CAKA;;AACA,UAAA,KAAA,SAAA,KAAA,MAAA,EAAA;AACA,QAAA,IAAA,GAAA,KAAA,QAAA;AACA,OAFA,MAEA;AACA,QAAA,IAAA,GAAA,KAAA,UAAA;AACA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,EAAA,EAAA,KAAA,MADA;AAEA,QAAA,KAAA,EAAA,KAAA,SAFA;AAGA,QAAA,WAAA,EAAA,KAAA,eAHA;AAIA,QAAA,SAAA,EAAA,KAAA,SAJA;AAKA,QAAA,IAAA,EAAA,IALA;AAMA,QAAA,QAAA,EAAA,KAAA,QANA;AAOA,QAAA,SAAA,EAAA,KAPA;AAQA,QAAA,OAAA,EAAA;AARA,OAAA;AAUA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EArBA,CAuBA;;AACA,WAAA,MAAA,IAAA,CAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,KAtCA;AAwCA,IAAA,UAxCA,sBAwCA,GAxCA,EAwCA;AAAA;AACA,MAAA,GAAA,CAAA,GAAA,GAAA,CAAA,GAAA,CAAA,GAAA;;AACA,UAAA,GAAA,CAAA,GAAA,EAAA;AACA,aAAA,QAAA,CAAA,IAAA,CACA,GAAA,CAAA,KADA;AAGA,OAJA,MAIA;AAAA;AACA,aAAA,QAAA,CAAA,MAAA,CAAA,GAAA,CAAA,KAAA,EAAA,CAAA;AACA;AACA,KAjDA;AAmDA,IAAA,kBAnDA,8BAmDA,OAnDA,EAmDA;AAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,KAAA,CAAA,OAAA,GAAA,EAAA,CAAA;AACA,UAAA,GAAA,GAAA,OAAA,GAAA,EAAA;AACA,aAAA,KAAA,GAAA,SAAA,GAAA,GAAA,GAAA,WAAA;AACA;AAvDA,GA9CA;AAwGA,EAAA,QAAA,EAAA;AAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,GAAA,GAAA,KAAA,SAAA,GAAA,EAAA,GAAA,KAAA,WAAA,GAAA,CAAA;AACA,aAAA,KAAA,kBAAA,CAAA,GAAA,CAAA;AACA,KAJA;AAKA,IAAA,UALA,wBAKA;AACA,UAAA,GAAA,GAAA,KAAA,YAAA,GAAA,KAAA,YAAA,GAAA,KAAA,mBAAA;AACA,aAAA,KAAA,kBAAA,CAAA,GAAA,CAAA;AACA;AARA;AAxGA,CAAA","sourcesContent":["<template>\r\n  <div class=\"content-wrapper\">\r\n    <section>\r\n      <div class=\"container\">\r\n        <h1 class=\"ui-title-1\">Home</h1>\r\n        <input v-model=\"taskTitle\" @keyup.enter=\"newTask\" type=\"text\" placeholder=\"Co będziesz oglądać\">\r\n        <textarea v-model=\"taskDescription\" @keyup.enter=\"newTask\" type=\"text\" ></textarea>\r\n        <div class=\"option-list\">\r\n          <input v-model='whatWatch' type=\"radio\" id=\"radioFilm\" value=\"Film\" class=\"what-watch--radio ui-checkbox\">\r\n          <label for=\"radioFilm\">Film</label>\r\n          <input v-model='whatWatch' type=\"radio\" id=\"radioSerial\" value=\"Serial\" class=\"what-watch--radio ui-checkbox\">\r\n          <label for=\"radioSerial\">Serial</label>\r\n        </div>\r\n        <div class=\"total-time\">\r\n          <div v-if=\"whatWatch === 'Film'\" class=\"total-time__film\">\r\n            <span class=\"time-title\">Godziny</span>\r\n            <input v-model=\"filmHours\" type=\"number\" class=\"time-input\">\r\n            <span class=\"time-title\">Minuty</span>\r\n            <input v-model=\"filmMinutes\" type=\"number\" class=\"time-input\">\r\n            <p> {{ filmTime }} </p>\r\n          </div>\r\n          <div v-if=\"whatWatch === 'Serial'\" class=\"total-time__serial\">\r\n            <span class=\"time-title\">Ile sezonów?</span>\r\n            <input v-model=\"serialSeason\" type=\"number\" class=\"time-input\">\r\n            <span class=\"time-title\">Ile odcinków?</span>\r\n            <input v-model=\"serialSeries\" type=\"number\" class=\"time-input\">\r\n            <span class=\"time-title\">Ile trwa jeden odcinek? (w minutach)</span>\r\n            <input v-model=\"serialSeriesMinutes\" type=\"number\" class=\"time-input\">\r\n            <p> {{ serialTime }} </p>\r\n          </div>\r\n        </div>\r\n        <div class=\"tag-list tag-list--add\">\r\n          <div @click=\"tagMenuShow = !tagMenuShow\" class=\"ui-tag__wrapper\">\r\n            <div class=\"ui-tag\">\r\n              <span class=\"tag-title\">Dodaj nowy</span>\r\n              <span :class=\"{ active: !tagMenuShow }\" class=\"button-close\"></span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-if=\"tagMenuShow\" class=\"tag-list tag-list--menu\">\r\n          <input v-model=\"tagTitle\" @keyup.enter=\"newTag\" type=\"text\" placeholder=\"Nowy tag\" class=\"tag-add--input\">\r\n          <div @click=\"newTag\" class=\"button button-default\">Wyślij</div>\r\n        </div>\r\n        <div class=\"tag-list\">\r\n          <div v-for=\"tag in tags\" :key=\"tag.title\" class=\"ui-tag__wrapper\">\r\n            <div @click=\"addTagUsed(tag)\" :class=\"{active: tag.use}\" class=\"ui-tag\">\r\n              <span class=\"tag-title\">{{ tag.title }}</span>\r\n              <span class=\"button-close\"></span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <p>{{ tagsUsed }}</p>\r\n        <div class=\"button-list\">\r\n          <div @click=\"newTask\" class=\"button button--round button-primary\">Wyślij</div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      taskTitle: '',\r\n      taskDescription: '',\r\n      whatWatch: 'Film',\r\n      taskId: 3,\r\n\r\n      // Dla czasu całkowitego\r\n      // Film\r\n      filmHours: 1,\r\n      filmMinutes: 30,\r\n      // Serial\r\n      serialSeason: 1,\r\n      serialSeries: 10,\r\n      serialSeriesMinutes: 50,\r\n\r\n      // Metki\r\n      tagTitle: '',\r\n      tagMenuShow: false,\r\n      tagsUsed: [],\r\n      tags: [\r\n        {\r\n          title: 'Komedia',\r\n          use: false\r\n        },\r\n        {\r\n          title: 'Horror',\r\n          use: false\r\n        },\r\n        {\r\n          title: 'Dramat',\r\n          use: false\r\n        },\r\n        {\r\n          title: 'Fantastyka',\r\n          use: false\r\n        },\r\n        {\r\n          title: 'Western',\r\n          use: false\r\n        }\r\n      ]\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    newTag () {\r\n      if (this.tagTitle === '') {\r\n        return\r\n      }\r\n      this.tags.push({\r\n        title: this.tagTitle,\r\n        used: false\r\n      })\r\n    },\r\n    newTask () { // Metoda do wpisywania danych w input\r\n      // Zabezpieczenie od zapisywania pustych form\r\n      if (this.taskTitle === '') {\r\n        return\r\n      }\r\n      let time // Wyswietlamy czas\r\n      if (this.whatWatch === 'Film') {\r\n        time = this.filmTime\r\n      } else {\r\n        time = this.serialTime\r\n      }\r\n      const task = {\r\n        id: this.taskId,\r\n        title: this.taskTitle,\r\n        description: this.taskDescription,\r\n        whatWatch: this.whatWatch,\r\n        time,\r\n        tagsUsed: this.tagsUsed,\r\n        completed: false,\r\n        editing: false\r\n      }\r\n      console.log(task)\r\n\r\n      // Resetujemy ID oraz nazwe z opisem w inpucie, wprowadzone dane w metki\r\n      this.taskId += 1\r\n      this.taskTitle = ''\r\n      this.taskDescription = ''\r\n      this.tagsUsed = []\r\n    },\r\n\r\n    addTagUsed (tag) { // Metoda do dodawania metek\r\n      tag.use = !tag.use\r\n      if (tag.use) {\r\n        this.tagsUsed.push(\r\n          tag.title\r\n        )\r\n      } else { // Usuniecie metek\r\n        this.tagsUsed.splice(tag.title, 1)\r\n      }\r\n    },\r\n\r\n    getHoursAndMinutes (minutes) { // Zaokraglamy minuty w godziny\r\n      const hours = Math.trunc(minutes / 60)\r\n      const min = minutes % 60\r\n      return hours + ' Hours ' + min + ' Minutes '\r\n    }\r\n  },\r\n\r\n  computed: { // Liczymy ilosc czasu dla kazdej kategorii\r\n    filmTime () {\r\n      const min = (this.filmHours * 60) + (this.filmMinutes * 1)\r\n      return this.getHoursAndMinutes(min)\r\n    },\r\n    serialTime () {\r\n      const min = this.serialSeason * this.serialSeries * this.serialSeriesMinutes\r\n      return this.getHoursAndMinutes(min)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n// Ustawienia wyboru\r\n.option-list\r\n  display flex\r\n  align-items center\r\n  margin-bottom 20px\r\n  .what-watch--radio\r\n    margin-right 12px\r\n  input\r\n    margin-bottom 0px\r\n  label\r\n    margin-right 20px\r\n    margin-bottom 0px\r\n    &:last-child\r\n      margin-right 0px\r\n\r\n// Czas całkowity\r\n.total-time\r\n  margin-bottom 20px\r\n.time-title\r\n  display block\r\n  margin-bottom 6px\r\n.time-input\r\n  max-width 80px\r\n  margin-right 10px\r\n  p\r\n    margin-bottom 6px\r\n  span\r\n    margin-right 16px\r\n  .task-input\r\n    max-width 80px\r\n    margin-bottom 28px\r\n    margin-right 10px\r\n\r\n// Metki\r\n.tag-list\r\n  margin-bottom 20px\r\n\r\n.ui-tag__wrapper\r\n  margin-right 18px\r\n  margin-bottom 10px\r\n  &:last-child\r\n    margin-right 0px\r\n\r\n.ui-tag\r\n  .button-close\r\n    &.active\r\n      transform rotate(45deg)\r\n  &.used\r\n    background-color #444ce0\r\n    color #fff\r\n    .button-close\r\n      &:before,\r\n      &:after\r\n        background-color #fff\r\n\r\n.tag-list--menu\r\n  display flex\r\n  justify-content space-between\r\n  align-items center\r\n.tag-add--input\r\n  margin-bottom 0px\r\n  margin-right 10px\r\n  height 42px\r\n.button-list\r\n  display flex\r\n  justify-content flex-end\r\n</style>\r\n"],"sourceRoot":"src/components"}]}